{
  "name": "react-inline-editable",
  "version": "0.0.0",
  "description": "A React component for inline editing",
  "type": "module",
  "main": "./dist/react-inline-editable.js",
  "module": "./dist/react-inline-editable.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react-inline-editable.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:docs": "vite build --config vite.config.docs.ts",
    "preview:docs": "vite preview --outDir dist-docs",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "react",
    "inline-edit",
    "editable",
    "inline-editing",
    "contenteditable",
    "edit-in-place"
  ],
  "author": "Subhash Malireddy <subhashmalireddy@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/subhash-malireddy/react-inline-editable.git"
  },
  "bugs": {
    "url": "https://github.com/subhash-malireddy/react-inline-editable/issues"
  },
  "homepage": "https://github.com/subhash-malireddy/react-inline-editable#readme",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.0.0"
  }
}
